{
  "name": "nuxt-study",
  "version": "1.0.0",
  "description": "My good Nuxt.js project",
  "author": "Lerni",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "styleguide": "vue-styleguidist server --config styleguide.config.js",
    "styleguide:build": "vue-styleguidist build --config styleguide.config.js"
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE >= 10"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "tailwindcss": "^1.2.0",
    "vue-cli-plugin-styleguidist": "^4.15.2",
    "vue-property-decorator": "^8.4.1",
    "vuex-module-decorators": "^0.16.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.1",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "cypress": "^4.3.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.9.1",
    "vue-styleguidist": "^4.15.2"
  }
}
